{
  "namespace": "settings",

  "settings_empty_overlay": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "bg": {
          "type": "image",
          "texture": "textures/ui/empty",
          "size": [ "100%", "100%" ],
          "layer": 0
        }
      }
    ]
  },

  "settings_common": {
    "settings_screen_content": {
      "type": "panel",
      "size": [ "100%", "100%" ]
    }
  },

  "settings_screen@common.base_screen": {
    "$screen_bg_content": "settings.settings_empty_overlay",
    "$screen_content": "settings.settings_common.settings_screen_content"
  }
}